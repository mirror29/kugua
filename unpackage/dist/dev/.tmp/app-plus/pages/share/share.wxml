<view class="container"><block wx:if="{{showSkeleton}}"><skeleton class="vue-ref" vue-id="1" loading="chiaroscuro" selector="skeleton" bgcolor="#FFF" data-ref="skeleton" bind:__l="__l"></skeleton></block><float class="vue-ref" vue-id="2" data-ref="float" bind:__l="__l"></float><modal class="vue-ref" vue-id="3" text="{{clipText}}" data-ref="modal" bind:__l="__l"></modal><mescroll-uni vue-id="4" up="{{upOption}}" tipShow="{{false}}" data-event-opts="{{[['^down',[['downCallback']]],['^up',[['upCallback']]],['^emptyclick',[['emptyClick']]],['^init',[['mescrollInit']]]]}}" bind:down="__e" bind:up="__e" bind:emptyclick="__e" bind:init="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="share"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="share-li shadow-warp"><view class="header"><view class="left"><image class="skeleton-radius" src="/static/images/logo.png"></image><view class="use"><text class="skeleton-rect">苦瓜推荐</text><text class="time skeleton-rect">{{item.time}}</text></view></view><view data-event-opts="{{[['tap',[['share',['$0'],[[['list','id',item.id]]]]]]]}}" class="btn skeleton-radius" bindtap="__e"><text class="icon"></text><text>分享</text></view></view><view class="text"><view class="title skeleton-rect"><block wx:if="{{item.type==='text'}}"><text>{{item.content}}</text></block><block wx:else><text><rich-text nodes="{{item.content}}"></rich-text></text></block></view></view><view class="img-list"><block wx:for="{{item.imgList}}" wx:for-item="item2" wx:for-index="i" wx:key="i"><view class="img skeleton-rect"><image src="{{item2}}" mode="widthFix" data-event-opts="{{[['tap',[['showImg',['$0'],[[['list','id',item.id]]]]]]]}}" bindtap="__e"></image></view></block></view></view></block></view></mescroll-uni></view>