<view class="register"><view class="content"><view class="header"><image src="../../static/images/kugua.png"></image></view><view class="main"><view class="main-list oBorder"><input class="main-input" type="text" maxlength="11" placeholder="手机号" data-event-opts="{{[['input',[['__set_model',['','phoneData','$event',[]]]]]]}}" value="{{phoneData}}" bindinput="__e"/></view><view class="main-list oBorder"><input class="main-input" type="text" maxlength="32" placeholder="登录密码" password="{{!showPassword}}" data-event-opts="{{[['input',[['__set_model',['','passData','$event',[]]]]]]}}" value="{{passData}}" bindinput="__e"/><image data-event-opts="{{[['tap',[['isShowPass',['$event']]]]]}}" class="{{['img',showPassword?'cuIcon-attention':'cuIcon-attentionforbid']}}" bindtap="__e"></image></view><view class="main-list oBorder"><input class="main-input" type="number" maxlength="4" placeholder="验证码" data-event-opts="{{[['input',[['__set_model',['','verCode','$event',[]]]]]]}}" value="{{verCode}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getVerCode']]]]}}" class="{{['vercode',[(second>0)?'vercode-run':'']]}}" bindtap="__e">{{getVerCodeSecond}}</view></view></view><button data-event-opts="{{[['tap',[['startReg']]]]}}" class="dlbutton buttonBorder" bindtap="__e">注 册</button><view class="footer"><text data-event-opts="{{[['tap',[['isShowAgree',['$event']]]]]}}" class="{{[showAgree?'cuIcon-radiobox':'cuIcon-round']}}" bindtap="__e">同意</text><navigator url open-type="navigate">《苦瓜用户协议》</navigator></view></view></view>