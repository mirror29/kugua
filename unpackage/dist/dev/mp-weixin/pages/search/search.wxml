<view class="container"><mescroll-uni vue-id="1" up="{{upOption}}" tipShow="{{false}}" data-event-opts="{{[['^down',[['downCallback']]],['^up',[['upCallback']]],['^emptyclick',[['emptyClick']]],['^init',[['mescrollInit']]]]}}" bind:down="__e" bind:up="__e" bind:emptyclick="__e" bind:init="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="search-tit"><image class="tit-bg" src="../../static/images/t25.png"></image><view class="tit-con"><label for><input type="text" placeholder="宝贝标题/关键词 （如：女装/男装）" data-event-opts="{{[['input',[['__set_model',['','keyword','$event',[]]]]]]}}" value="{{keyword}}" bindinput="__e"/><text data-event-opts="{{[['tap',[['toGoodsList',['$0'],['keyword']]]]]}}" bindtap="__e">搜索</text></label></view></view><view class="search-hot"><text class="hot-tit">热门搜索</text><view class="hot-con"><block wx:for="{{hotList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoodsList',['$0'],[[['hotList','',index,'name']]]]]]]}}" class="hot-list" bindtap="__e">{{item.name}}</view></block></view></view><view class="guess-like"><view class="like-tit">— 猜你喜欢 —</view><view class="like-con"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['list','',index,'origin_id']]]]]]]}}" class="like-li" bindtap="__e"><image src="{{item.pict_url}}" lazy-load></image><view class="like-foot"><text class="like-foot-tit">{{item.title}}</text><view class="foot-con"><view class="like-left"><view class="like-le-price">{{"原价￥"+item.zk_final_price}}</view><view class="get-quan"><image src="/static/images/icon/quan.png"></image><text class="like-le-quan">券<text>{{"减"+item.reduce}}</text></text></view></view><view class="like-right"><text class="like-ri-price">{{"券后价￥"+item.afterPrice}}</text><text class="like-sales">{{"销量"+item.volume}}</text></view></view></view></view></block></view></view></mescroll-uni></view>