<view><view class="content"><image class="title" src="../../../static/images/withdraw.png"></image><view class="main"><view data-event-opts="{{[['tap',[['toPayList',['$event']]]]]}}" class="main-list oBorder" bindtap="__e"><text class="icon text-blue"></text><input class="main-input" type="number" maxlength="11" placeholder="请先绑定支付宝" disabled="true" data-event-opts="{{[['input',[['__set_model',['','phoneData','$event',[]]]]]]}}" value="{{phoneData}}" bindinput="__e"/><text class="icon text-grey"></text></view><view class="main-list oBorder"><text class="icon text-grey"></text><input class="main-input" type="digit" placeholder="请输入提现金额" data-event-opts="{{[['input',[['__set_model',['','amountData','$event',[]]]]]]}}" value="{{amountData}}" bindinput="__e"/></view><view class="main-list oBorder"><text class="icon text-grey"></text><input class="main-input" type="number" maxlength="4" placeholder="验证码" data-event-opts="{{[['input',[['__set_model',['','verCode','$event',[]]]]]]}}" value="{{verCode}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getVerCode']]]]}}" class="{{['vercode',[(second>0)?'vercode-run':'']]}}" bindtap="__e">{{getVerCodeSecond}}</view></view></view><button data-event-opts="{{[['tap',[['startWithdraw',['$event']]]]]}}" class="dlbutton buttonBorder" bindtap="__e">确定</button></view></view>