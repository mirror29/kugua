<view class="container"><float class="vue-ref" vue-id="1" data-ref="float" bind:__l="__l"></float><modal class="vue-ref" vue-id="2" text="{{clipText}}" data-ref="modal" bind:__l="__l"></modal><view class="search-tit"><image class="tit-bg" src="../../static/images/search-bg.png"></image><view class="tit-con"><label for><input type="text" placeholder="{{placeholder}}" data-event-opts="{{[['input',[['__set_model',['','keyword','$event',[]]]]]]}}" value="{{keyword}}" bindinput="__e"/><text data-event-opts="{{[['tap',[['toGoodsList']]]]}}" bindtap="__e">搜索</text></label></view></view><block wx:if="{{oldKeywordList.length>0}}"><view class="search-hot"><text class="hot-tit">历史记录</text><view class="hot-con"><block wx:for="{{oldKeywordList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoodsList2',['$0'],[[['oldKeywordList','',index]]]]]]]}}" class="hot-list" bindtap="__e">{{item}}</view></block></view></view></block><view class="search-hot"><text class="hot-tit">热门搜索</text><view class="hot-con"><block wx:for="{{hotList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoodsList2',['$0'],[[['hotList','',index,'keyword']]]]]]]}}" class="hot-list" bindtap="__e">{{item.keyword}}</view></block></view></view><list class="list" vue-id="3" list="{{list}}" bind:__l="__l"></list></view>