<view class="pd-list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="pd-li"><image class="pd-img" src="{{item.item_img||'/static/images/noImg.png'}}" mode="widthFix" lazy-load></image><view class="pd-name"><block wx:if="{{item.order_type==1}}"><image src="../../static/images/tianmao.png" mode lazy-load></image></block><block wx:else><image src="../../static/images/tb.png" mode lazy-load></image></block>{{''+item.item_title}}</view><view class="order-num"><text>{{"订单号："+item.trade_parent_id}}</text><text data-event-opts="{{[['tap',[['copy',['$0'],[[['list','id',item.id,'trade_parent_id']]]]]]]}}" class="copy" bindtap="__e">复制</text></view><block wx:if="{{item.settlement_commission==='0.00'}}"><view class="order-time">{{"创建日期:"+item.tk_create_time}}</view></block><block wx:else><view class="order-time">{{"结算日期:"+item.tk_earning_time}}</view></block><block wx:if="{{item.tk_status!='13'}}"><view class="order-price"><text class="payment">实付：<text>{{item.payment_price}}</text></text><block wx:if="{{item.settlement_commission==='0.00'}}"><text class="estimate">{{"预估佣金："+item.estimate_commission}}</text></block><block wx:else><text class="estimate">{{"结算佣金："+item.settlement_commission}}</text></block></view></block><view class="order-user"><text class="order-level">{{item.order_level}}<text>级订单</text></text><text class="order-use">所属用户：<text>{{item.from_mobile}}</text></text></view></view></block></view>